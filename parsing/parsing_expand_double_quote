export b=world
echo ""
echo sa"lut"
echo "sa""lut"
echo "sa" "lut"
echo "$a"
echo "$b"
echo "$a$b"
echo "$a salut $b"
echo "$a"salut"$b"
echo "$1var"
echo "$1salut"
echo "$1"'salut'
echo "$1""salut"
echo "$42"salut
